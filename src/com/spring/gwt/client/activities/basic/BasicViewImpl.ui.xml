<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:w="urn:import:com.spring.gwt.client.activities.basic.widgets"
	>
	<ui:style>
	</ui:style>
	<g:HTMLPanel ui:field="mainPanel" styleName="basic-page" width="100%" height="100%">
		<w:HeaderPanel ui:field="headerPanel"></w:HeaderPanel>
		<div class="body-panel">
			<div class="tabs-panel container">
				<b:Button ui:field="buttonHome" text="Trang chủ" icon="HOME"></b:Button>
				<b:Button ui:field="buttonProducts" text="Sản phẩm" icon="PRODUCT_HUNT"></b:Button>
				<b:Button ui:field="buttonSettings" text="Cài đặt" icon="COGS"></b:Button>
				<b:Button ui:field="buttonProcess" text="Tiến trình" icon="LINE_CHART"></b:Button>
			</div>
			<g:FlowPanel ui:field="contentPanel" styleName="container"></g:FlowPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 