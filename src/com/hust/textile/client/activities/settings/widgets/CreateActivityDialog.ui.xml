<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:v="urn:import:com.hust.textile.client.view"
	>
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<div class="row">
			<div class="col-xs-12 col-sm-6">
				<label>Tên</label>
				<b:TextBox ui:field="nameBox"></b:TextBox>
			</div>
			<div class="col-xs-12 col-sm-6">
				<label>TM</label>
				<b:TextBox ui:field="tmBox"></b:TextBox>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12 col-sm-6">
				<label>TP</label>
				<b:TextBox ui:field="tpBox"></b:TextBox>
			</div>
			<div class="col-xs-12 col-sm-6">
				<label>Chọn Actions</label>
				<v:HMultipleSelect ui:field="selectActions"></v:HMultipleSelect>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				<label>Mô tả</label>
				<b:TextArea ui:field="descriptionBox"></b:TextArea>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 